{
  "name": "obsidian-scrippets",
  "version": "1.1.1",
  "description": "Run and manage custom JavaScript scrippets from your vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint ./src",
    "check": "tsc -noEmit -skipLibCheck && npm run lint",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" styles.css \"*.md\"",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepublish:assets": "node -e \"const fs=require('fs');if(!fs.existsSync('main.js')){process.exitCode=1;throw new Error('Run npm run build first');}\"",
    "release": "node scripts/release.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "typescript-eslint": "^8.44.1",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "obsidian": "1.8.7",
    "prettier": "^3.6.2",
    "tslib": "^2.6.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {}
}
