{
  "name": "obsidian-scrippets",
  "version": "1.1.0",
  "description": "Run and manage custom JavaScript scrippets from your vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "prepublish:assets": "node -e \"const fs=require('fs');if(!fs.existsSync('main.js')){process.exitCode=1;throw new Error('Run npm run build first');}\"",
    "release": "node scripts/release.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint": "^8.57.1",
    "globals": "^16.4.0",
    "obsidian": "1.8.7",
    "tslib": "^2.6.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {}
}
